  andBool (#rangeAddress(Owner)
  andBool (#rangeUInt(256, Gem_d)
  andBool (#rangeUInt(256, Supply)
  andBool (#rangeBool(Stopped)
  andBool ((#sizeByteArray(CD) <=Int 1250000000)
  andBool ((#notPrecompileAddress(Owner))
